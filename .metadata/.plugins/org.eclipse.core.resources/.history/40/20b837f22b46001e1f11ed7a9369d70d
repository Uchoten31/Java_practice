package javaBasic1;

public class Q115 {
	public static void main(String[] args) {
        // 1. int配列型変数の宣言
		int[] nums;
        // 2. 配列オブジェクトの生成
		nums = new int[3];
        // 3. 各要素の値を代入
		nums[0] = 25;
		nums[1] = 39;
		nums[2] = 43;
        // 4. 各要素の値を表示
		for(int i = 0; i < nums.length; i++) {
			System.out.println(nums[i]);
		}
    }
}
